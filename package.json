{
  "name": "safe-any",
  "version": "1.0.1",
  "description": "Type-safe alternative to TypeScript any.",
  "author": "Victorien Elvinger (victorien.elvinger.fr)",
  "contributors": [
    "Matthieu Nicolas"
  ],
  "main": "index.d.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsc --noEmit",
    "lint": "tslint -p tsconfig.json && tslint -p code-samples/tsconfig.json",
    "test": "tsc -p code-samples/ --noEmit && npm run lint",
    "release": "standard-version --sign --",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Conaclos/safe-any.git"
  },
  "keywords": [
    "typescript",
    "safe-any",
    "structural-test"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Conaclos/safe-any/issues"
  },
  "homepage": "https://github.com/Conaclos/safe-any#readme",
  "types": "index.d.ts",
  "devDependencies": {
    "husky": "^0.14.3",
    "standard-version": "^4.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "validate-commit-msg": "^2.14.0"
  }
}
